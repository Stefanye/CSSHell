  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stefanye/CSSHell/issues"
  },
  "homepage": "https://github.com/Stefanye/CSSHell#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "node-sass": "^5.0.0"
  }
}
